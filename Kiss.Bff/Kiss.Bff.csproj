    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>fd21c90f-fa7c-49b2-af82-4089ee78a973</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
